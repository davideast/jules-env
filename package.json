{
  "name": "@_davideast/jules-env",
  "version": "0.1.2",
  "description": "Configure ephemeral development environments",
  "license": "Apache-2.0",
  "type": "module",
  "main": "dist/cli.mjs",
  "bin": {
    "jules-env": "./bin/jules-env"
  },
  "files": [
    "bin/",
    "dist/",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/node": "^22",
    "commander": "^14.0.2",
    "zod": "3.23.8"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "scripts": {
    "build": "bun run build:node && bun run build:binary",
    "build:node": "bun build ./src/cli.ts --outfile dist/cli.mjs --target node",
    "build:binary": "bun build ./src/cli.ts --compile --outfile jules-env",
    "start": "bun run ./src/cli.ts",
    "test": "bun test",
    "typecheck": "tsc --noEmit",
    "check:all": "bun run scripts/prepublish-checks.ts",
    "prepack": "bun run scripts/prepublish-checks.ts",
    "docker": "docker build -t jules-env . && docker run --rm -it jules-env"
  }
}
